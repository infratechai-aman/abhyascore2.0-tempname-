        <question>
            <text>Graph of PV vs P for ideal gas:</text>
            <option id="A">Horizontal line parallel to P axis</option><option id="B">Slope</option><option id="C">Curve</option><option id="D">Origin</option>
            <answer>A</answer><explanation>Constant.</explanation>
        </question>
