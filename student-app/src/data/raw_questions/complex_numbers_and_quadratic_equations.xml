            <answer>A</answer><explanation>Square to check.</explanation>
        </question>
        <question>
            <text>Arg(z) + Arg(conjugate z) = ?</text>
            <option id="A">0</option><option id="B">pi</option><option id="C">2pi</option><option id="D">pi/2</option>
