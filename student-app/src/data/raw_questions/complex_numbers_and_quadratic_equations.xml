            <answer>A</answer><explanation>Complex plane.</explanation>
        </question>
        <question>
            <text>Value of i^3 is:</text>
            <option id="A">-i</option><option id="B">i</option><option id="C">1</option><option id="D">-1</option>
            <answer>A</answer><explanation>i^2 * i = -1 * i.</explanation>
        </question>
