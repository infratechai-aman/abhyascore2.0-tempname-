<chapter subject="Mathematics" id="205" name="Complex Numbers and Quadratic Equations">
    <easy>
        <question>
            <text>Value of i^4 is:</text>
            <option id="A">1</option><option id="B">-1</option><option id="C">i</option><option id="D">-i</option>
            <answer>A</answer><explanation>i^2 = -1, i^4 = 1.</explanation>
        </question>
        <question>
            <text>Conjugate of 3 + 4i is:</text>
            <option id="A">3 - 4i</option><option id="B">3 + 4i</option><option id="C">-3 - 4i</option><option id="D">-3 + 4i</option>
            <answer>A</answer><explanation>Change sign of imaginary part.</explanation>
        </question>
        <question>
            <text>Modulus of 5 - 12i is:</text>
            <option id="A">13</option><option id="B">17</option><option id="C">5</option><option id="D">12</option>
            <answer>A</answer><explanation>sqrt(25 + 144) = 13.</explanation>
        </question>
        <question>
            <text>Additive inverse of 2 - 3i is:</text>
            <option id="A">-2 + 3i</option><option id="B">2 + 3i</option><option id="C">-2 - 3i</option><option id="D">2 - 3i</option>
            <answer>A</answer><explanation>Negative of complex number.</explanation>
        </question>
        <question>
            <text>Real part of (2+3i) + (1-2i) is:</text>
            <option id="A">3</option><option id="B">2</option><option id="C">1</option><option id="D">4</option>
            <answer>A</answer><explanation>2+1 = 3.</explanation>
        </question>
        <question>
            <text>Imaginary part of (2+3i) - (1-i) is:</text>
            <option id="A">4</option><option id="B">2</option><option id="C">3</option><option id="D">-1</option>
            <answer>A</answer><explanation>3 - (-1) = 4.</explanation>
        </question>
        <question>
            <text>Multiplicative inverse of 1+i is:</text>
            <option id="A">(1-i)/2</option><option id="B">1-i</option><option id="C">(1+i)/2</option><option id="D">-1-i</option>
            <answer>A</answer><explanation>Conjugate divided by modulus squared.</explanation>
        </question>
        <question>
            <text>Value of i^2 is:</text>
            <option id="A">-1</option><option id="B">1</option><option id="C">i</option><option id="D">0</option>
            <answer>A</answer><explanation>Definition.</explanation>
        </question>
        <question>
            <text>Standard form of complex number:</text>
            <option id="A">a + ib</option><option id="B">a - ib</option><option id="C">a * ib</option><option id="D">None</option>
            <answer>A</answer><explanation>Real + Imaginary.</explanation>
        </question>
        <question>
            <text>Roots of x^2 + 1 = 0 are:</text>
            <option id="A">+i, -i</option><option id="B">1, -1</option><option id="C">0, 1</option><option id="D">None</option>
            <answer>A</answer><explanation>Definition of i.</explanation>
        </question>
        <question>
            <text>Solve x^2 + 4 = 0:</text>
            <option id="A">+2i, -2i</option><option id="B">+2, -2</option><option id="C">+4i, -4i</option><option id="D">+i, -i</option>
            <answer>A</answer><explanation>x = sqrt(-4).</explanation>
        </question>
        <question>
            <text>Argand plane uses axes:</text>
