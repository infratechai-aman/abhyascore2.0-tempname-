<?xml version="1.0" encoding="UTF-8"?>
<chapter subject="Mathematics" id="204" name="Linear Inequalities">
    <easy>
        <question>
            <text>Solve x + 5 > 7:</text>
            <option id="A">x > 2</option><option id="B">x &lt; 2</option><option id="C">x > 12</option><option id="D">x &lt; 12</option>
            <answer>A</answer><explanation>Subtract 5 from both sides.</explanation>
        </question>
        <question>
            <text>Solve 3x &lt; 9:</text>
            <option id="A">x &lt; 3</option><option id="B">x > 3</option><option id="C">x &lt; 27</option><option id="D">x > 27</option>
            <answer>A</answer><explanation>Divide by positive 3.</explanation>
        </question>
        <question>
            <text>Solve -2x > 4:</text>
            <option id="A">x &lt; -2</option><option id="B">x > -2</option><option id="C">x &lt; 2</option><option id="D">x > 2</option>
            <answer>A</answer><explanation>Divide by negative flips sign.</explanation>
        </question>
        <question>
            <text>Solve x - 3 &lt;= 2:</text>
            <option id="A">x &lt;= 5</option><option id="B">x >= 5</option><option id="C">x &lt;= 1</option><option id="D">x >= 1</option>
            <answer>A</answer><explanation>Add 3.</explanation>
        </question>
        <question>
            <text>Solution set of x for Natural numbers, if x &lt; 4:</text>
            <option id="A">{1, 2, 3}</option><option id="B">{0, 1, 2, 3}</option><option id="C">{1, 2, 3, 4}</option><option id="D">{...}</option>
            <answer>A</answer><explanation>Strict inequality.</explanation>
        </question>
        <question>
            <text>If x is an integer and 3x &lt; 11, largest value is:</text>
            <option id="A">3</option><option id="B">2</option><option id="C">4</option><option id="D">10</option>
            <answer>A</answer><explanation>11/3 = 3.66. Max integer is 3.</explanation>
        </question>
        <question>
            <text>Graph of x > 2 on number line:</text>
            <option id="A">Open circle at 2, arrow right</option><option id="B">Closed circle at 2, arrow right</option><option id="C">Open circle at 2, arrow left</option><option id="D">Closed circle, left</option>
            <answer>A</answer><explanation>Strict inequality means open.</explanation>
        </question>
        <question>
            <text>Strict inequalities use symbols:</text>
            <option id="A">&lt;, ></option><option id="B">&lt;=, >=</option><option id="C">=</option><option id="D">!=</option>
            <answer>A</answer><explanation>Definition.</explanation>
        </question>
        <question>
            <text>Slack inequalities use symbols:</text>
            <option id="A">&lt;=, >=</option><option id="B">&lt;, ></option><option id="C">=</option><option id="D">None</option>
            <answer>A</answer><explanation>Allow equality.</explanation>
        </question>
        <question>
            <text>Multiplying inequality by negative number:</text>
            <option id="A">Reverses sign</option><option id="B">Keeps sign</option><option id="C">Makes it zero</option><option id="D">Error</option>
            <answer>A</answer><explanation>Fundamental rule.</explanation>
        </question>
        <question>
            <text>Solve 5x - 3 &lt; 7:</text>
            <option id="A">x &lt; 2</option><option id="B">x > 2</option><option id="C">x &lt; 4</option><option id="D">x > 4</option>
            <answer>A</answer><explanation>5x &lt; 10 -> x &lt; 2.</explanation>
        </question>
        <question>
            <text>Solve 3x + 8 > 2:</text>
            <option id="A">x > -2</option><option id="B">x &lt; -2</option><option id="C">x > 2</option><option id="D">x &lt; 2</option>
            <answer>A</answer><explanation>3x > -6 -> x > -2.</explanation>
        </question>
        <question>
            <text>Solve 4x + 3 &lt; 6x + 7:</text>
            <option id="A">x > -2</option><option id="B">x &lt; -2</option><option id="C">x > 2</option><option id="D">x &lt; 2</option>
            <answer>A</answer><explanation>-2x &lt; 4 -> x > -2.</explanation>
        </question>
        <question>
            <text>Solve (3x - 4)/2 >= (x+1)/4:</text>
            <option id="A">x >= 1.8</option><option id="B">x &lt;= 1.8</option><option id="C">x >= 2</option><option id="D">x &lt;= 2</option>
            <answer>A</answer><explanation>2(3x-4) >= x+1 -> 6x-8 >= x+1 -> 5x >= 9 -> x >= 1.8.</explanation>
        </question>
        <question>
            <text>Interval notation for x >= 3:</text>
            <option id="A">[3, infinity)</option><option id="B">(3, infinity)</option><option id="C">(-infinity, 3]</option><option id="D">[3, infinity]</option>
            <answer>A</answer><explanation>Square bracket for inclusive.</explanation>
        </question>
        <question>
            <text>Interval notation for -2 &lt; x &lt;= 5:</text>
            <option id="A">(-2, 5]</option><option id="B">[-2, 5)</option><option id="C">[-2, 5]</option><option id="D">(-2, 5)</option>
            <answer>A</answer><explanation>Mixed bound.</explanation>
        </question>
        <question>
            <text>Solve -12x > 30:</text>
            <option id="A">x &lt; -2.5</option><option id="B">x > -2.5</option><option id="C">x &lt; 2.5</option><option id="D">x > 2.5</option>
            <answer>A</answer><explanation>Divide by -12.</explanation>
        </question>
        <question>
            <text>If x is real, solve x &lt; 5x - 2:</text>
            <option id="A">x > 0.5</option><option id="B">x &lt; 0.5</option><option id="C">x > 2</option><option id="D">x &lt; 2</option>
            <answer>A</answer><explanation>-4x &lt; -2 -> x > 0.5.</explanation>
        </question>
        <question>
            <text>Solve x/3 > x/2 + 1:</text>
            <option id="A">x &lt; -6</option><option id="B">x > -6</option><option id="C">x &lt; 6</option><option id="D">x > 6</option>
            <answer>A</answer><explanation>Multiply by 6: 2x > 3x + 6 -> -x > 6 -> x &lt; -6.</explanation>
        </question>
        <question>
            <text>Region x >= 0, y >= 0 lies in:</text>
            <option id="A">First quadrant</option><option id="B">Second</option><option id="C">Third</option><option id="D">Fourth</option>
            <answer>A</answer><explanation>Positive axes.</explanation>
        </question>
        <question>
            <text>Line x = 2 is parallel to:</text>
            <option id="A">Y-axis</option><option id="B">X-axis</option><option id="C">y = x</option><option id="D">None</option>
            <answer>A</answer><explanation>Vertical line.</explanation>
        </question>
        <question>
            <text>Line y = 3 is parallel to:</text>
            <option id="A">X-axis</option><option id="B">Y-axis</option><option id="C">y = x</option><option id="D">None</option>
            <answer>A</answer><explanation>Horizontal line.</explanation>
        </question>
        <question>
            <text>Check if (0,0) satisfies 2x + 3y &lt;= 6:</text>
            <option id="A">Yes</option><option id="B">No</option><option id="C">Maybe</option><option id="D">Error</option>
            <answer>A</answer><explanation>0 + 0 &lt;= 6 is true.</explanation>
        </question>
        <question>
            <text>Solution region of x > -3 includes origin?</text>
            <option id="A">Yes</option><option id="B">No</option><option id="C">On boundary</option><option id="D">None</option>
            <answer>A</answer><explanation>0 > -3 is true.</explanation>
        </question>
        <question>
            <text>Inequality y &lt; 2 represents:</text>
            <option id="A">Half plane below y=2</option><option id="B">Half plane above y=2</option><option id="C">Line y=2</option><option id="D">None</option>
            <answer>A</answer><explanation>Region.</explanation>
        </question>
        <question>
            <text>Dashed line is used for:</text>
            <option id="A">Strict inequality</option><option id="B">Slack inequality</option><option id="C">Equality</option><option id="D">None</option>
            <answer>A</answer><explanation>Boundary not included.</explanation>
        </question>
        <question>
            <text>Solid line is used for:</text>
            <option id="A">Slack inequality (>=, &lt;=)</option><option id="B">Strict</option><option id="C">None</option><option id="D">Always</option>
            <answer>A</answer><explanation>Boundary included.</explanation>
        </question>
        <question>
            <text>Absolute value inequality |x| &lt; a means:</text>
            <option id="A">-a &lt; x &lt; a</option><option id="B">x &lt; -a or x > a</option><option id="C">x &lt; a</option><option id="D">x > -a</option>
            <answer>A</answer><explanation>Distance from origin less than a.</explanation>
        </question>
        <question>
            <text>Solve |x| > 5:</text>
            <option id="A">x &lt; -5 or x > 5</option><option id="B">-5 &lt; x &lt; 5</option><option id="C">x > 5</option><option id="D">x &lt; -5</option>
            <answer>A</answer><explanation>Distance greater than 5.</explanation>
        </question>
        <question>
            <text>Ravi scored 70 and 75 in first two tests. Needed average 60. Third test score x?</text>
            <option id="A">x >= 35</option><option id="B">x >= 60</option><option id="C">x &lt;= 35</option><option id="D">x >= 40</option>
            <answer>A</answer><explanation>(70+75+x)/3 >= 60 -> 145+x >= 180 -> x >= 35.</explanation>
        </question>
        <question>
            <text>Length of rectangle is x, breadth is 20. Perimeter &lt; 100. Range of x?</text>
            <option id="A">0 &lt; x &lt; 30</option><option id="B">x &lt; 30</option><option id="C">x > 30</option><option id="D">x &lt; 50</option>
            <answer>A</answer><explanation>2(x+20) &lt; 100 -> x+20 &lt; 50 -> x &lt; 30. Also length > 0.</explanation>
        </question>
        <question>
            <text>Marks must be between:</text>
            <option id="A">0 and 100</option><option id="B">-infinity to infinity</option><option id="C">0 to 50</option><option id="D">None</option>
            <answer>A</answer><explanation>Standard percentage.</explanation>
        </question>
        <question>
            <text>Cost C(x) = 20x + 4000. Revenue R(x) = 40x. Profit P(x) > 0 implies:</text>
            <option id="A">x > 200</option><option id="B">x &lt; 200</option><option id="C">x > 100</option><option id="D">x &lt; 100</option>
            <answer>A</answer><explanation>40x > 20x + 4000 -> 20x > 4000 -> x > 200.</explanation>
        </question>
        <question>
            <text>IQ = MA/CA * 100. If 80 &lt;= IQ &lt;= 140 for 12 year old (CA=12), range of MA?</text>
            <option id="A">9.6 &lt;= MA &lt;= 16.8</option><option id="B">8 &lt;= MA &lt;= 14</option><option id="C">10 &lt;= MA &lt;= 16</option><option id="D">None</option>
            <answer>A</answer><explanation>80 &lt;= MA/12*100 &lt;= 140. 0.8*12 &lt;= MA &lt;= 1.4*12.</explanation>
        </question>
        <question>
            <text>x + y &lt; 5. Point (2,2) satisfies?</text>
            <option id="A">Yes</option><option id="B">No</option><option id="C">Boundary</option><option id="D">Error</option>
            <answer>A</answer><explanation>4 &lt; 5.</explanation>
        </question>
        <question>
            <text>If -3x + 2 >= -5, then:</text>
            <option id="A">x &lt;= 7/3</option><option id="B">x >= 7/3</option><option id="C">x &lt;= -1</option><option id="D">x >= -1</option>
            <answer>A</answer><explanation>-3x >= -7 -> x &lt;= 7/3.</explanation>
        </question>
        <question>
            <text>1/x &lt; 1 implies:</text>
            <option id="A">x > 1 or x &lt; 0</option><option id="B">x > 1</option><option id="C">x &lt; 1</option><option id="D">x > 0</option>
            <answer>A</answer><explanation>(1-x)/x &lt; 0. Signs of numerator and denominator.</explanation>
        </question>
        <question>
            <text>Meaning of "at least":</text>
            <option id="A">>=</option><option id="B">&lt;=</option><option id="C">></option><option id="D">&lt;</option>
            <answer>A</answer><explanation>Definition.</explanation>
        </question>
        <question>
            <text>Meaning of "at most":</text>
            <option id="A">&lt;=</option><option id="B">>=</option><option id="C">></option><option id="D">&lt;</option>
            <answer>A</answer><explanation>Definition.</explanation>
        </question>
        <question>
            <text>If x is positive, then x > 0?</text>
            <option id="A">Yes</option><option id="B">No</option><option id="C">x >= 0</option><option id="D">None</option>
            <answer>A</answer><explanation>Strictly positive.</explanation>
        </question>
    </easy>
    <medium>
        <question>
            <text>Solution of |x| &lt; 3 is:</text>
            <option id="A">(-3, 3)</option><option id="B">[-3, 3]</option><option id="C">(-inf, -3) U (3, inf)</option><option id="D">(3, inf)</option>
            <answer>A</answer><explanation>Between -3 and 3.</explanation>
        </question>
        <question>
            <text>Solve |x - 1| &lt;= 5:</text>
            <option id="A">[-4, 6]</option><option id="B">[-5, 5]</option><option id="C">[6, 12]</option><option id="D">[-6, 4]</option>
            <answer>A</answer><explanation>-5 &lt;= x-1 &lt;= 5. Add 1 -> -4 &lt;= x &lt;= 6.</explanation>
        </question>
        <question>
            <text>Solve |2x - 3| > 7:</text>
            <option id="A">x &lt; -2 or x > 5</option><option id="B">x &lt; -5 or x > 2</option><option id="C">-2 &lt; x &lt; 5</option><option id="D">None</option>
            <answer>A</answer><explanation>2x-3 > 7 or 2x-3 &lt; -7. x > 5 or x &lt; -2.</explanation>
        </question>
        <question>
            <text>Solve 5x - 3 &lt; 3x + 1 when x is integer:</text>
            <option id="A">... -1, 0, 1</option><option id="B">1, 2, 3 ...</option><option id="C">... -2, -1</option><option id="D">0, 1, 2</option>
            <answer>A</answer><explanation>2x &lt; 4 -> x &lt; 2.</explanation>
        </question>
        <question>
            <text>Solve 37 - (3x + 5) >= 9x - 8(x - 3):</text>
            <option id="A">x &lt;= 2</option><option id="B">x >= 2</option><option id="C">x &lt;= 8</option><option id="D">x >= 8</option>
            <answer>A</answer><explanation>32 - 3x >= 9x - 8x + 24 -> 32 - 3x >= x + 24 -> 8 >= 4x -> 2 >= x.</explanation>
        </question>
        <question>
            <text>Solve 2(2x+3) - 10 &lt; 6(x-2):</text>
            <option id="A">x > 4</option><option id="B">x &lt; 4</option><option id="C">x > 2</option><option id="D">x &lt; 2</option>
            <answer>A</answer><explanation>4x+6-10 &lt; 6x-12 -> 4x-4 &lt; 6x-12 -> 8 &lt; 2x -> 4 &lt; x.</explanation>
        </question>
        <question>
            <text>System of inequalities: 3x - 7 > 2(x - 6) and 6 - x > 11 - 2x. Range?</text>
            <option id="A">x > 5</option><option id="B">x > -5</option><option id="C">x &lt; 5</option><option id="D">x &lt; -5</option>
            <answer>A</answer><explanation>3x-7 > 2x-12 -> x > -5. 6-x > 11-2x -> x > 5. Intersection is x > 5.</explanation>
        </question>
        <question>
            <text>Solve -5 &lt;= (5 - 3x)/2 &lt;= 8:</text>
            <option id="A">[-11/3, 5]</option><option id="B">[5, 11/3]</option><option id="C">[-5, 11/3]</option><option id="D">None</option>
            <answer>A</answer><explanation>-10 &lt;= 5-3x &lt;= 16. -15 &lt;= -3x &lt;= 11. 5 >= x >= -11/3.</explanation>
        </question>
        <question>
            <text>How many liters of 30% acid to be added to 600L of 12% acid to get mixture > 15% but &lt; 18%?</text>
            <option id="A">120 &lt; x &lt; 300</option><option id="B">x > 300</option><option id="C">x &lt; 120</option><option id="D">60 &lt; x &lt; 150</option>
            <answer>A</answer><explanation>Mixture problem linear inequality.</explanation>
        </question>
        <question>
            <text>Graphical solution of x + y &lt; 5:</text>
            <option id="A">Dotted line through (5,0) and (0,5), region towards origin</option><option id="B">Solid line, away from origin</option><option id="C">Dotted line, away from origin</option><option id="D">Solid line, towards origin</option>
            <answer>A</answer><explanation>Test point (0,0) works.</explanation>
        </question>
        <question>
            <text>Graph of 2x - y > 1:</text>
            <option id="A">Region containing (2,0)</option><option id="B">Region containing (0,0)</option><option id="C">Line itself</option><option id="D">Region containing (0,2)</option>
            <answer>A</answer><explanation>Check (0,0): 0 > 1 False. (2,0): 4 > 1 True.</explanation>
        </question>
        <question>
            <text>Solution of (x - 1)(x - 2) > 0:</text>
            <option id="A">(-inf, 1) U (2, inf)</option><option id="B">(1, 2)</option><option id="C">[1, 2]</option><option id="D">R</option>
            <answer>A</answer><explanation>Outside roots.</explanation>
        </question>
        <question>
            <text>Solution of x^2 &lt; 4:</text>
            <option id="A">(-2, 2)</option><option id="B">(-inf, -2) U (2, inf)</option><option id="C">[-2, 2]</option><option id="D">{2, -2}</option>
            <answer>A</answer><explanation>Between roots.</explanation>
        </question>
        <question>
            <text>Solution of 1/x > 2:</text>
            <option id="A">(0, 0.5)</option><option id="B">x > 0.5</option><option id="C">x &lt; 0.5</option><option id="D">(-inf, 0.5)</option>
            <answer>A</answer><explanation>(1-2x)/x > 0. Roots 0, 0.5. Coeff neg. Positive interval (0, 0.5).</explanation>
        </question>
        <question>
            <text>Solve x^2 - 3x + 2 &lt;= 0:</text>
            <option id="A">[1, 2]</option><option id="B">(1, 2)</option><option id="C">(-inf, 1]</option><option id="D">[2, inf)</option>
            <answer>A</answer><explanation>(x-1)(x-2) &lt;= 0.</explanation>
        </question>
        <question>
            <text>Region enclosed by |x| + |y| &lt;= 1:</text>
            <option id="A">Square with vertices (±1, 0), (0, ±1)</option><option id="B">Circle radius 1</option><option id="C">Triangle</option><option id="D">Square side 1</option>
            <answer>A</answer><explanation>Distance metric L1.</explanation>
        </question>
        <question>
            <text>Longest side of triangle is 3 times shortest. Third is 2cm shorter than longest. Perimeter >= 61. Min shortest side?</text>
            <option id="A">9</option><option id="B">10</option><option id="C">8</option><option id="D">7</option>
            <answer>A</answer><explanation>x + 3x + (3x-2) >= 61. 7x >= 63. x >= 9.</explanation>
        </question>
        <question>
            <text>Water acidity must be between 7.5 and 8.5 pH. If mixture of pH x and pH y ... Wait pH is log. Simple linear assumed? Usually concentration.</text>
            <option id="A">Context dependent</option><option id="B">Linear</option><option id="C">Logarithmic</option><option id="D">Skip</option>
            <answer>A</answer><explanation>Generally linear mass balance.</explanation>
        </question>
        <question>
            <text>Solution of system x+y &lt;= 6, x+y >= 4:</text>
            <option id="A">Strip between lines</option><option id="B">Empty</option><option id="C">All plane</option><option id="D">Single line</option>
            <answer>A</answer><explanation>Region between parallel lines.</explanation>
        </question>
        <question>
            <text>Solve (x+3)/(x-2) >= 0:</text>
            <option id="A">(-inf, -3] U (2, inf)</option><option id="B">[-3, 2)</option><option id="C">[-3, 2]</option><option id="D">(-3, 2)</option>
            <answer>A</answer><explanation>Roots -3, 2. Open at 2.</explanation>
        </question>
        <question>
            <text>Solve x^2 &lt; -1:</text>
            <option id="A">Empty set</option><option id="B">R</option><option id="C">0</option><option id="D">Imaginary</option>
            <answer>A</answer><explanation>Square is non-negative real.</explanation>
        </question>
        <question>
            <text>Solve |x - 1| >= -2:</text>
            <option id="A">R</option><option id="B">Empty</option><option id="C">x >= 1</option><option id="D">x &lt;= 1</option>
            <answer>A</answer><explanation>Modulus always >= 0, so always >= -2.</explanation>
        </question>
        <question>
            <text>If x &lt; 5, then -x > -5:</text>
            <option id="A">True</option><option id="B">False</option><option id="C">Maybe</option><option id="D">Error</option>
            <answer>A</answer><explanation>Multiplying by -1 reverses inequality.</explanation>
        </question>
        <question>
            <text>Solve x > 0, y > 0, x+y &lt; 0:</text>
            <option id="A">No solution</option><option id="B">Origin</option><option id="C">Q1</option><option id="D">Q3</option>
            <answer>A</answer><explanation>Sum of positive numbers is positive.</explanation>
        </question>
        <question>
            <text>Number of integer solutions for |x| &lt; 5:</text>
            <option id="A">9</option><option id="B">10</option><option id="C">11</option><option id="D">8</option>
            <answer>A</answer><explanation>-4, -3, -2, -1, 0, 1, 2, 3, 4.</explanation>
        </question>
        <question>
            <text>Solve (2x - 1)/3 >= (3x - 2)/4 - (2 - x)/5:</text>
            <option id="A">x >= 2</option><option id="B">x &lt;= 2</option><option id="C">x >= -2</option><option id="D">x &lt;= -2</option>
            <answer>A</answer><explanation>Calculation.</explanation>
        </question>
        <question>
            <text>Solve 4x + 3 >= 2x + 17, 3x - 5 &lt; -2:</text>
            <option id="A">No common solution</option><option id="B">x = 1</option><option id="C">x = 7</option><option id="D">R</option>
            <answer>A</answer><explanation>2x >= 14 -> x >= 7. 3x &lt; 3 -> x &lt; 1. No overlap.</explanation>
        </question>
        <question>
            <text>Solve 5(2x - 7) - 3(2x + 3) &lt;= 0, 2x + 19 &lt;= 6x + 47:</text>
            <option id="A">-7 &lt;= x &lt;= 11</option><option id="B">x &lt;= 11</option><option id="C">x >= -7</option><option id="D">None</option>
            <answer>A</answer><explanation>10x-35-6x-9 &lt;= 0 -> 4x &lt;= 44 -> x &lt;= 11. -28 &lt;= 4x -> -7 &lt;= x.</explanation>
        </question>
        <question>
            <text>Which point lies in region 3x + 2y - 12 > 0?</text>
            <option id="A">(5, 5)</option><option id="B">(0, 0)</option><option id="C">(1, 1)</option><option id="D">(2, 2)</option>
            <answer>A</answer><explanation>15 + 10 - 12 = 13 > 0. (0,0) is -12 &lt; 0.</explanation>
        </question>
        <question>
            <text>Region for x - y &lt;= 2 is:</text>
            <option id="A">Contains (0,0)</option><option id="B">Does not contain (0,0)</option><option id="C">Boundary</option><option id="D">Line</option>
            <answer>A</answer><explanation>0 - 0 &lt;= 2 True.</explanation>
        </question>
